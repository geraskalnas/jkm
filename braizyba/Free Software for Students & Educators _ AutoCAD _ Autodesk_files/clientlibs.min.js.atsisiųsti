if(typeof h==="undefined"){h=this.options.dataAttributeDefaults[i]
}if(g){var e=this.dataAttributeAllowedParameters[i];
if(!e.hasOwnProperty(h)){var f=this.errors.UNKNOWN_SETTING[i].replace("%ATTR%",h);
console.warn(f)
}h=e[h]||false
}return h
};
return b
});
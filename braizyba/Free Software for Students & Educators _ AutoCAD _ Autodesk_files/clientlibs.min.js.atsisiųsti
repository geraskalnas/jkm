};
b.prototype.removeClassnames=function(e){c(e).removeClass(this.options.classNames.loading);
c(e).removeClass(this.options.classNames.done);
c(e).removeClass(this.options.classNames.error)
};
b.prototype.replace=function(g,f,e){if(typeof e==="undefined"){c(g).text(f)
}else{c(g).attr(e,f)
}};
b.prototype.getDataAttributeValue=function(j,i,g){g=(typeof g!=="undefined")?!!g:true;

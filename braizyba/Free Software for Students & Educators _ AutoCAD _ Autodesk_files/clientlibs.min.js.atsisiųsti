define("adsk.ui.replacer",["jquery","adsk.data.base"],function(c,a){var d={classNames:{loading:"replacer-loading",done:"replacer-done",error:"replacer-error"},dataAttributes:{target:"replacer-target"},autoInitialize:false};
function b(e){if(!(this instanceof b)){throw new Error(this.errors.USE_NEW_KEYWORD)
}this.options=c.extend(true,{},d,e);
if(this.options.autoInitialize===true){this.init()
}}b.prototype=Object.create(a.prototype);
b.prototype.constructor=b;
b.prototype.errors=c.extend(a.errors,{});
b.prototype.errors.UNKNOWN_SETTING={target:"adsk.ui.Replacer: the requested target parameter (%ATTR%) does not exist in the response (data-pds-target)"};
b.prototype.dataAttributeAllowedParameters={};
b.prototype.init=function(){var e=this;
this.getPlaceholders().each(function(){c(this).on("adsk.ui.Replacer.refreshPlaceholder",e.refreshPlaceholder.bind(e,this)).trigger("adsk.ui.Replacer.refreshPlaceholder")
})
};
b.prototype.getPlaceholders=function(){return c("[data-"+this.options.dataAttributes.target+"]")
};
b.prototype.refreshPlaceholder=function(f){var e=this;
this.removeClassnames(f);
this.request(f).done(function(g){e.onSuccess(g,f)
}).fail(function(g){e.onFailure(g,f)
})
};
b.prototype.request=function(f){c(f).addClass(this.options.classNames.loading);
if(this.options.data){var e=new c.Deferred();
return e.resolve(this.options.data).promise()
}};
b.prototype.lookup=function(h,g,i,f){if(typeof h==="undefined"){var e=this.errors.UNKNOWN_SETTING.target.replace("%ATTR%",g);
console.warn(e);
c(i).addClass(this.options.classNames.error)
}else{c(i).addClass(this.options.classNames.done);
if(typeof h==="object"){h=JSON.stringify(h)
}this.replace(i,h,f)
}};
b.prototype.onSuccess=function(e,i){var f=this.getDataAttributeValue(i,"target",false);
var g=this.dataAttributeAllowedParameters.target||{};
var h=g[f]||f;
this.removeClassnames(i);
this.lookup(e[h],h,i)
};
b.prototype.onFailure=function(f,e){console.warn("adsk.ui.Replacer: "+f.responseJSON.responseText||"error");
this.removeClassnames(e);
